{
  "manifest_version": 3,
  "name": "No YouTube Shorts",
  "description": "Convert YouTube shorts to normal YouTube video.",
  "version": "1.0",
  "author": "Monmcgt",
  "host_permissions": [
    "https://*.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "src/inject.js"
      ],
      "css": [
        "src/inject.css"
      ]
    }
  ],
  "action": {}
}
